{
  "name": "elegant-wedding-invitations",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ng serve",
    "build": "ng build --output-path=dist --base-href=/wedding/",
    "copy-404": "cp dist/browser/index.html dist/browser/404.html",
    "publish-gh": "npx gh-pages -d ./dist/browser",
    "deploy": "npm run build && npm run copy-404 && npm run publish-gh"
   
  },
  "dependencies": {
    "rxjs": "^7.8.2",
    "@angular/platform-browser": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}